@inherits UmbracoViewPage
<head>
    @if (System.Configuration.ConfigurationManager.AppSettings["redirectToHttps"] == "true")
    {
        <script>
            if (window.location.protocol != 'https:') {
                location.href = location.href.replace("http://", "https://");
            }
        </script>
    }

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="copyright" content="Miroslav Podmajersky" />
    <meta name="description" content="@Model.Value("metaDescription")" />
    <meta name="language" content="sk" />
    <meta name="content-language" content="sk" />
    <meta name="author" content="Miroslav Podmajersky" />
    <meta name="robots" content="index, noodp" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href="@Url.Content("~/Styles/images/MediavelTimes1.png")" rel="shortcut icon" />

    <title>@Model.Value("pageTitle")</title>

    <link href="@Url.Content("~/Styles/css/bootstrap.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Styles/bootstrap-theme.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Styles/css/stylymojhowebu.css")" type="text/css" rel="stylesheet" />


    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>

</head>
